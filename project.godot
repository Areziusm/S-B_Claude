;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Sortilèges & Bestioles"
config/description="Un RPG narratif dans l'univers du Disque-Monde de Terry Pratchett"
run/main_scene="uid://blgfxkd0n0ieb"
config/features=PackedStringArray("4.4", "Forward Plus")
boot_splash/bg_color=Color(0.11, 0.11, 0.13, 1)
boot_splash/fullsize=false
boot_splash/use_filter=false
boot_splash/image="res://assets/ui/splash_logo.png"
config/icon="res://icon.svg"

[autoload]

Game="*res://scripts/managers/GameManager.gd"
Data="*res://scripts/managers/DataManager.gd"
Observation="*res://scripts/managers/ObservationManager.gd"
Dialogue="*res://scripts/managers/DialogueManager.gd"
Quest="*res://scripts/managers/QuestManager.gd"
Reputation="*res://scripts/managers/ReputationSystem.gd"
Combat="*res://scripts/managers/CombatSystem.gd"
UI="*res://scripts/ui/UIManager.gd"
Audio="*res://scripts/managers/AudioManager.gd"

[debug]

settings/crash_handler/message="Sortilèges & Bestioles a rencontré un problème. Veuillez sauvegarder et redémarrer."
settings/stdout/verbose_stdout=true
gdscript/warnings/untyped_declaration=1
gdscript/warnings/inferred_declaration=1
gdscript/warnings/unsafe_property_access=1
gdscript/warnings/unsafe_method_access=1
gdscript/warnings/unsafe_cast=1
gdscript/warnings/unsafe_call_argument=1
gdscript/warnings/treat_warnings_as_errors=false

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PackedStringArray()

[file_customization]

folder_colors={
"res://assets/": "blue",
"res://data/": "green",
"res://scenes/": "yellow",
"res://scripts/": "red"
}

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

[gui]

theme/custom="res://assets/ui/themes/main_theme.tres"
theme/custom_font="res://assets/fonts/pratchett_font.ttf"
common/drop_mouse_on_gui_input_disabled=true

